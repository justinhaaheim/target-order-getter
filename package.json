{
  "name": "target-order-getter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "dev": "tsx src/getTargetOrderData.ts",
    "clean:build": "rm -rf build/",
    "clean:node-modules": "rm -rf node_modules/",
    "lint:fix": "npm run lint --fix",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier-check": "prettier --check .",
    "prettier": "prettier --write .",
    "signal:serial": "npm run ts-check && npm run lint && npm run prettier-check",
    "signal": "concurrently --names \"TS,LINT,PRETTIER\" -c \"bgBlack.bold,bgRed.bold,bgCyan.bold\" \"npm run ts-check\" \"npm run lint\" \"npm run prettier-check\"",
    "ts-check": "tsc --noEmit",
    "tsc": "tsc --noEmit",
    "ts-check-masked": "npm run ts-check",
    "login": "op run --env-file .env.local -- playwright test",
    "test": "jest src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@playwright/test": "1.46.1",
    "@types/uuid": "10.0.0",
    "dayjs": "1.11.12",
    "nullthrows": "1.1.1",
    "playwright": "1.46.1",
    "tsx": "4.17.0",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@types/fill-range": "7.0.3",
    "@types/jest": "29.5.12",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "concurrently": "8.2.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "ts-jest": "29.2.4",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "**/*": "prettier --check --ignore-unknown",
    "*.(js|jsx|cjs|ts|tsx)": [
      "eslint --cache --max-warnings=0"
    ]
  }
}
